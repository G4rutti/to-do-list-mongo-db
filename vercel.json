{
  "name": "controlador-de-gastos",
  "public": true,
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "Cache-Control": "s-maxage=1, stale-while-revalidate"
      }
    }
  ]
}